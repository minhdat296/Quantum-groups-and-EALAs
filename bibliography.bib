@article{nayak_essentially_of_finite_type,
    title = {Compactification for essentially finite-type maps},
    journal = {Advances in Mathematics},
    volume = {222},
    number = {2},
    pages = {527-546},
    year = {2009},
    issn = {0001-8708},
    doi = {https://doi.org/10.1016/j.aim.2009.05.002},
    url = {https://www.sciencedirect.com/science/article/pii/S0001870809001364},
    author = {Suresh Nayak},
    keywords = {Nagata compactification, Essentially of finite type, Grothendieck duality, Localizing map, Birational map},
    abstract = {We show that any separated essentially finite-type map f of noetherian schemes globally factors as f=hi where i is an injective localization map and h a separated finite-type map. In particular, via Nagata's compactification theorem, h can be chosen to be proper. We apply these results to Grothendieck duality. We also obtain other factorization results and provide essentialized versions of many general results such as Zariski's Main Theorem, Chow's Lemma, and blow-up descriptions of birational maps.}
}

